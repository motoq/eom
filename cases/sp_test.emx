
#
# Loads ephemeris for ETALON-2 from an SP3 format file and compare to
# propagated ephemeris
#
# 2023/01/16
#

SimStart GD 2019  2 3  0  0  0.00;
#SimStart GD 2019  2 6  9  0  0.00;
SimDuration Days 6.75;
#SimDuration Days 1.75;
LeapSeconds 37;
EcfEciRate Minutes 240;

EphemerisFile sp3_sat  SP3c Hermite nsgf.orb.etalon2.190209.v70.sp3;

CelestialEphemerides moon sun mercury venus mars
                              jupiter saturn neptune uranus pluto;

DistanceUnits  Kilometers;
TimeUnits Seconds;
PropagatorConfig  gj      GravityModel  Standard 12 12
                          MoonGravity  Ephemeris
                          SunGravity   Ephemeris
                          SRP Spherical 0.8 0.000125
                          Integrator  GJ Seconds 0;
PropagatorConfig  gjs     GravityModel  Standard 12 12
                          MoonGravity  Ephemeris
                          SunGravity   Ephemeris
                          SRP Spherical 0.8 0.000125
                          Integrator  GJs Seconds 0;
PropagatorConfig  adams4  GravityModel  Standard 12 12
                          MoonGravity  Ephemeris
                          SunGravity   Ephemeris
                          SRP Spherical 0.8 0.000125
                          Integrator  Adams4 Seconds 0;
PropagatorConfig  rk4     GravityModel  Standard 12 12
                          MoonGravity  Ephemeris
                          SunGravity   Ephemeris
                          SRP Spherical 0.8 0.000125
                          Integrator  RK4 Seconds 0;
Orbit  rk4_sat     SP  GD 2019  2 6  9  0  0.00
       CART  ITRF  -3335.613077  17015.323947 -18697.175676
                   -1.5845243000  2.0240798000  2.1159881000
                   Propagator rk4;
Orbit  adams4_sat  SP  GD 2019  2 6  9  0  0.00
       CART  ITRF  -3335.613077  17015.323947 -18697.175676
                   -1.5845243000  2.0240798000  2.1159881000
                   Propagator adams4;
Orbit  gj_sat      SP  GD 2019  2 6  9  0  0.00
       CART  ITRF  -3335.613077  17015.323947 -18697.175676
                   -1.5845243000  2.0240798000  2.1159881000
                   Propagator gj;
Orbit  gjs_sat     SP  GD 2019  2 6  9  0  0.00
       CART  ITRF  -3335.613077  17015.323947 -18697.175676
                   -1.5845243000  2.0240798000  2.1159881000
                   Propagator gjs;
TimeUnits Minutes;
OutputRate Minutes 5;
Command PrintRange sp3_sat rk4_sat    rk4_rng;
Command PrintRange sp3_sat adams4_sat adams4_rng;
Command PrintRange sp3_sat gj_sat     gj_rng;
Command PrintRange sp3_sat gjs_sat    gjs_rng;

end;

